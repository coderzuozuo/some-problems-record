<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxl.problems.migratespringboot.dao.UsersDao">
    <select id="isExistsUsernameTokenPair" resultType="boolean">
        select exists(select 1 from users where username = #{0} and token = #{1} limit 1)
    </select>
</mapper>